<body>
    <script>
      // 1、定义一个 user 对象，对象中存在 id name age bir sex order对象
      // order对象中有 id name address

      var user = {
          id:"12",
          name:"刘雨昕",
          age:23,
          bir:"2020-02-02",
          sex:true,
          order:{
              id:"22222",
              name:"巴适",
              address:"西安"
          }
      };
      // 访问属性
      console.log(user.name);
      // 获取对象中对象
      console.log(typeof user.order);
      user.order.name = "土豆";
      console.log(user.order);
    </script>
</body>
