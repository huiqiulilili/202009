public boolean collisWith (int x1, int y1, int r1, int x2, int y2, int r2) {
        // write code here
        int r = Math.abs(r1 + r2);
        double tmp = Math.pow((x1-x2),2)+Math.pow(y1-y2,2);
        double jiange = Math.pow(tmp,(double) 1/2);
        if(jiange <= r) {
            return true;
        }else {
            return false;
        }
    }
