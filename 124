@RequestMapping("/a2")
    public List<User> test3() {
        ArrayList<User> users = new ArrayList<User>();
        User user1 = new User("张三1", 23, "nv1");
        User user2 = new User("张三2", 23, "nv2");
        User user3 = new User("张三3", 23, "nv3");
        users.add(user1);
        users.add(user2);
        users.add(user3);
        return users;
    }
